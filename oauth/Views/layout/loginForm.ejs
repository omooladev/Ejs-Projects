<form class="auth_form">
  <h1>Log in</h1>
  <a class="google" href="/auth/google">
    <span class="icon"><i class="bx bxl-google"></i></span>
    <p>Use Google Account</p></a
  >
  <div class="or">
    <hr />
    <p>Or</p>
  </div>
  <p class="reply">This is the reply</p>
  <div class="form_controls">
    <div class="form_control">
      <label for="username_input">Username</label>
      <input type="text" class="auth_username_input" id="username_input" name="username" />
    </div>
    <div class="form_control">
      <label for="password_input">Password</label>
      <div class="password_container">
        <input type="password" class="auth_password_input" id="password_input" name="password" />
        <div class="eye_container">
          <i class="bx bxs-show icon eye-show"></i>
          <i class="bx bxs-hide icon eye-hide"></i>
        </div>
      </div>
    </div>
  </div>
  <div class="form_actions">
    <button type="submit" class="login-button">Log In</button>
  </div>
  <div class="form_footer">
    <p>Don't have an account? <a href="/auth/signup">Sign Up</a></p>
  </div>
</form>
